<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.mustafa.codexhud.hello</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/zsh</string>
        <string>/Users/Mustafa/DevFiles/Mac Apps/codex-hud/scripts/run-daily-hello.sh</string>
    </array>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>6</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StartInterval</key>
    <integer>600</integer>
    <key>StandardOutPath</key>
    <string>/tmp/codex-hud-hello.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/codex-hud-hello.err</string>
    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
